for (p=24; p < 64; p += 8) {
    base = 2^p;
    limit = base + 512;
    for (q=nextcand(base+256); q < limit; q=nextcand(q)) {
	printf("%x %d %b\n", q, p, q-base);
    }
    limit = base + 65536;
    for (q=nextcand(base+32768); q < limit; q=nextcand(q)) {
	printf("%x %d %b\n", q, p, q-base);
    }
}
